    <Compile Update="ViewModel\Checkout\Delivery.xaml.cs">
      <DependentUpon>Delivery.xaml</DependentUpon>
    </Compile>
    <Compile Update="ViewModel\Subscribe\PaymentOptionPage.xaml.cs">
